{
  "name": "turbo-sandbox",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "commit": "git cz"
  },
  "devDependencies": {
    "git-cz": "^4.9.0",
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}